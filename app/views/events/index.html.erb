
<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Artist</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.artist %></td>
        <td><%= event.event_date %></td>
        <td><%= link_to 'Details', event %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
 
 <%= link_to 'New event', new_event_path %>